<%#= f.input :publisher,  input_html: { class: 'ubiquity-publisher form-control' }, required: f.object.required?(key) %>
<% my_model_name = f.object.model.class.to_s.underscore %>
<div class="<%= my_model_name %>_publisher form-group">
  <label class="control-label" for="<%= my_model_name %>_publisher">Publisher</label>
  <p class='help-block'>
    <% if my_model_name == 'collection' %>
      <%= t('simple_form.hints.collection.publisher') %>
    <% else %>
      <%= t('simple_form.hints.defaults.publisher') %>
    <% end %>
  </p>
  <%= text_field_tag "#{my_model_name}[publisher][]", f.object.model.publisher.first, id: "#{my_model_name}_publisher",
    class: "form-control ubiquity-publisher" %>
</div>