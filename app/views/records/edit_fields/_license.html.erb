<% my_model_name = f.object.model.class.to_s.underscore %>
<div class="<%= my_model_name %>_license form-group">
  <%= f.input :license,
              as: :multi_value_select,
              collection: ::LicenseService.select_active_options,
              include_blank: true,
              item_helper: ::LicenseService.method(:include_current_value),
              input_html: { class: 'form-control ubiquity-license' } %>
</div>
